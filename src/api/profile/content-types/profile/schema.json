    "vision": {
      "type": "component",
      "repeatable": false,
      "component": "profile.vision",
      "required": true
    },
    "mission": {
      "type": "component",
      "repeatable": false,
      "component": "profile.mission",
      "required": true
    },
    "history": {
      "type": "component",
      "repeatable": false,
      "component": "profile.history",
      "required": true
    },
    "company_structure": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "company_value": {
      "type": "component",
      "repeatable": false,
      "component": "profile.company-value",
      "required": true
