      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Dewan Komisaris",
        "Dewan Direksi",
        "Kepala Divisi"
      ],
      "required": true
    },
    "short_description": {
      "type": "text"
    }
