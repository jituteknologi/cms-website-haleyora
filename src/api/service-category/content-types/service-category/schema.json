{
  "kind": "collectionType",
  "collectionName": "service_categories",
  "info": {
    "singularName": "service-category",
    "pluralName": "service-categories",
    "displayName": "Master Service Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "service_category"
    }
  }
}
